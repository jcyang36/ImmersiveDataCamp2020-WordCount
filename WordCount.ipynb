    "def countWords():\n",
    "    filename=\"king.txt\"\n",
    "    with open(filename,encoding='utf-8') as f_obj:\n",
    "        contents=f_obj.read()\n",
    "    words = contents.split()\n",
    "    num_words=len(words)\n",
    "    \n",
    "    print(\"The file \"+filename+\" has \" +str(num_words)+\" words.\")\n",
    "    \n"
